# -*- coding: utf-8; mode: conf; -*-

[report]
exclude_lines =
    pragma: no cover
    if __name__ == '__main__':
    def __repr__
    @nb\.vectorize
    @nb\.njit
    @nb\.jit
ignore_errors = True

[run]
omit =
    */csv_visualize/__main__.py
    */csv_visualize/command_input.py
    */csv_visualize/errors.py
    */csv_visualize/native_parallel.py
source = csv_visualize

[html]
show_contexts = True