    |   |---- connect_peer()
    |   |---- move_storage()
    |   |---- pause_session()
    |   |---- resume_session()
    |   |---- is_session_paused()
    |   |---- resume_torrent()
    |   |---- resume_torrents()
    |   |---- get_torrent_status()
